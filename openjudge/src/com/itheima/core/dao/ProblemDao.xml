	             	  #{id},
	                  #{title},
	                  #{desc},
	                  #{indesc},
	                  #{outdesc},
 					  #{iss},
	                  #{hint},

	                  #{checknamee},
	                  #{create_time},

	                  #{time_limit},
	                  #{memory_limit},
	                  #{typee},
	                  #{vis},
	                  #{difficulty},

	                  #{source},

	                  #{created_by_id},

	                  #{iomodee},
	                  #{ss}
	            )
